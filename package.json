{
  "name": "n8n-nodes-aem-ops",
  "version": "0.1.0",
  "description": "n8n community nodes for Adobe Experience Manager (AEM) operations - Health checks, replication, package management, and cache purging",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "aem",
    "adobe",
    "experience-manager",
    "cms",
    "devops",
    "automation"
  ],
  "license": "BUSL-1.1",
  "homepage": "https://velobpa.com",
  "author": {
    "name": "Velocity BPA, LLC",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://velobpa.com"
  },
  "engines": {
    "node": ">=18.10",
    "npm": ">=9.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && npm run copy-icons",
    "copy-icons": "copyfiles -u 2 \"src/nodes/**/*.svg\" dist/nodes",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/Aem65Credentials.credentials.js",
      "dist/credentials/AemCloudCredentials.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AemHealthCheck/AemHealthCheck.node.js",
      "dist/nodes/AemReplicate/AemReplicate.node.js",
      "dist/nodes/AemPackage/AemPackage.node.js",
      "dist/nodes/AemCachePurge/AemCachePurge.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
